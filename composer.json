{
  "name": "in2code/luxletter-import",
  "description": "Imports luxletter receivers to TYPO3 from a csv file",
  "keywords": [
    "typo3",
    "newsletter",
    "luxletter"
  ],
  "homepage": "https://in2code.de",
  "authors": [
    {
      "name": "Bastien Lutz",
      "email": "bastien.lurt@in2code.de",
      "role": "Developer",
      "homepage": "https://www.in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4.0",
    "typo3/cms-core": "^11.5",
    "in2code/luxletter": "^23.0"
  },
  "autoload": {
    "psr-4": {
      "In2code\\LuxletterImport\\": "Classes/"
    }
  },
  "require-dev": {
    "typo3/cms-core": "^11.5",
    "typo3/cms-extbase": "^11.5",
    "typo3/cms-extensionmanager": "^11.5",
    "typo3/cms-felogin": "^11.5",
    "typo3/cms-filelist": "^11.5",
    "typo3/cms-fluid-styled-content": "^11.5",
    "typo3/cms-frontend": "^11.5",
    "typo3/cms-info": "^11.5",
    "typo3/cms-install": "^11.5",
    "typo3/cms-recordlist": "^11.5",
    "typo3/cms-rte-ckeditor": "^11.5",
    "typo3/cms-scheduler": "^11.5",
    "typo3/cms-setup": "^11.5",
    "typo3/cms-t3editor": "^11.5",
    "typo3/cms-tstemplate": "^11.5",
    "typo3/cms-lowlevel": "^11.5",
    "typo3/cms-adminpanel": "^11.5",
    "typo3/cms-belog": "^11.5",
    "typo3/cms-beuser": "^11.5",
    "typo3/cms-dashboard": "^11.5",
    "typo3/testing-framework": "dev-main",
    "symfony/config": "^6.2",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.8",
    "friendsofphp/php-cs-fixer": "^3.10",
    "helmich/typo3-typoscript-lint": "^3.1",
    "helhum/typo3-console": "^8.0.x-dev"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/public",
      "extension-key": "luxletter_import",
      "ignore-as-root": false
    }
  }
}
